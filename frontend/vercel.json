{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/marketplace/(.*)",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/marketplace/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://api.nibiru.com",
    "NEXT_PUBLIC_DEMO_MODE": "true"
  }
} 