  "preDev": [
    {
      "id": "detect-route-conflicts",
      "script": "scripts/detect-route-conflicts.js",
      "description": "Scans for duplicate route definitions",
      "stopOnError": false
    }
  ],
  "scripts": {
    "dev": "scripts/start-fixed-server.js",
    "clean": "scripts/direct-fix.js",
    "check-routes": "scripts/detect-route-conflicts.js"
  },
  "aiRoles": {
    "Sphinx": "Architect, moral alignment, macro vision",
    "Claude": "Executor, systems builder, file sync",
    "Cursor": "Local mutation agent, sandbox ops"
  }
} 