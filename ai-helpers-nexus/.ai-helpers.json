{
  "version": "1.0",
  "rootLock": true,
  "portRequired": 3000,
  "errorFixes": [
    {
      "id": "nextjs-to-arg-type",
      "script": "scripts/next-watchpack-fix.js",
      "status": "active",
      "appliesTo": ["next@14.x", "next@15.x"],
      "trigger": "TypeError [ERR_INVALID_ARG_TYPE]: The \"to\" argument must be of type string"
    }
